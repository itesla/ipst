<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>%d{yyyy-MM-dd_HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</Pattern>
        </encoder>
    </appender>

    <!--<logger name="com.gridquant.helm.analysis.lf.core.LF30" level="WARN"/>-->
    <!--<logger name="com.gridquant.helm.analysis.lf.core" level="WARN"/>-->
    <logger name="IpsoProblemDefinitionService" level="INFO"/>

    <root level="WARN">
        <appender-ref ref="STDOUT" />
        <!--<appender-ref ref="FILE" />-->
    </root>
</configuration>